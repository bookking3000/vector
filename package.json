{
  "name": "vector",
  "version": "2.0.0",
  "description": "A library for creating interactive vector graphics on the web.",
  "author": "Kurt Bruns",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "unpkg": "dist/vector.min.js",
  "jsdeliver": "dist/vector.min.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "clean": "rm -rf dist/ lib/ node_modules/",
    "clean-install": "yarn clean && yarn",
    "deploy": "echo \"Error: no test specified\" && exit 1",
    "package": "tar --exclude=\"*.js.map\" -czvf ./dist/library.tar.gz ./dist/ ./css/library.css",
    "rollup": "rollup --config",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "jest",
    "tsc": "tsc --declaration true --sourceMap false --skipLibCheck",
    "watch": "tsc -w",
    "website": "cd website && hugo"
  },
  "jest": {
    "testMatch": [
      "**/test/*.test.js"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "homepage": "https://vectorjs.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/vector-js/vector.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "vector.js",
    "dist/elements/",
    "dist/model/",
    "dist/util/",
    "dist/index.js",
    "dist/interactive.js",
    "css/"
  ],
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@rollup/plugin-babel": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.2",
    "mini-css-extract-plugin": "^0.11.3",
    "rollup": "^2.28.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-loader": "^8.0.4",
    "tslib": "^2.0.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
